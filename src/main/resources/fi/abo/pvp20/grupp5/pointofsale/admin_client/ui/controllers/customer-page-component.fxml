<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.VBox?>
<VBox spacing="12.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1"
      fx:id="customerPage"
      fx:controller="fi.abo.pvp20.grupp5.pointofsale.admin_client.ui.controllers.CustomerPageComponentController">
    <children>
        <Label fx:id="nameLabel" text="Name" styleClass="title"/>
        <Label fx:id="idLabel" text="Id"/>
        <Label fx:id="detailsLabel" text="Details"/>
        <Label fx:id="addressLabel" text="Address"/>
        <Label text="Most purchased products"/>
        <ListView fx:id="productListView" maxHeight="100"/>
        <Label fx:id="cardsLabel" text="Cards"/>
    </children>
    <padding>
        <Insets bottom="16.0" left="16.0" right="16.0" top="16.0"/>
    </padding>
</VBox>
