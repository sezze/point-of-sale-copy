<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ListView?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>
<VBox prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1"
      fx:controller="fi.abo.pvp20.grupp5.pointofsale.cashier_client.ui.controllers.CatalogTabController">
    <children>
        <HBox spacing="8.0">
            <children>
                <TextField fx:id="productNameField" promptText="Product name" HBox.hgrow="ALWAYS"/>
                <Button fx:id="searchButton" mnemonicParsing="false" text="Search"/>
            </children>
            <opaqueInsets>
                <Insets/>
            </opaqueInsets>
            <padding>
                <Insets bottom="8.0" left="8.0" right="8.0" top="8.0"/>
            </padding>
        </HBox>
        <ListView fx:id="productListView" prefHeight="200.0" prefWidth="200.0" VBox.vgrow="ALWAYS"/>
    </children>
</VBox>
